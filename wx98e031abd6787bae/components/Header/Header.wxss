.header,.header .null {
    width: 100%;
}

.header .null image {
    height: 100%;
    width: 100%;
}

.header .back-btn {
    height: 100%;
    position: relative;
    width: 100rpx;
    z-index: 99;
}

.header .show {
    bottom: 0;
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    width: 100%;
    z-index: 999;
}

.header .show .status-bar {
    width: 100%;
}

.header .show .header-container {
    display: flex;
    position: relative;
    width: 100%;
}

.header .show .header-container .title {
    font-size: 32rpx;
    font-weight: 500;
    left: 50%;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%);
}

