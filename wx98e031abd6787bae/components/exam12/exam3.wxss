.close {
    align-items: center;
    background: #ffcf00;
    border-radius: 12rpx;
    color: #333;
    display: flex;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 32rpx;
    font-weight: 500;
    height: 92rpx;
    justify-content: center;
    margin-top: 64rpx;
    width: 100%;
}

.jump .xian2:nth-child(1) {
    animation: dong 1s linear infinite;
    background: #979797;
    bottom: 0;
    left: 0;
    position: absolute;
}

.jump .xian2:nth-child(2) {
    animation: dong .3s linear infinite;
    background: #979797;
    bottom: 0;
    left: 8rpx;
    position: absolute;
}

.jump .xian2:nth-child(3) {
    animation: dong 1.2s linear infinite;
    background: #979797;
    bottom: 0;
    left: 16rpx;
    position: absolute;
}

.jump .xian2:nth-child(4) {
    animation: dong 1.5s linear infinite;
    background: #979797;
    bottom: 0;
    left: 24rpx;
    position: absolute;
}

@-webkit-keyframes dong {
    0% {
        height: 2rpx;
    }

    50% {
        height: 20rpx;
    }

    100% {
        height: 10rpx;
    }
}

@keyframes dong {
    0% {
        height: 2rpx;
    }

    50% {
        height: 20rpx;
    }

    100% {
        height: 10rpx;
    }
}

