    width: 50rpx;
}

.off {
    color: #fff;
    font-size: 26rpx;
    font-weight: 700;
    position: absolute;
    right: 8rpx;
    top: 11rpx;
}

.off,.yin {
    font-family: PingFang SC;
}

.yin {
    color: #434343;
    font-size: 30rpx;
