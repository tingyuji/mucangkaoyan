}

.list {
    color: #959595;
    display: inline-block;
    font-size: 30rpx;
    font-weight: 500;
    line-height: 97rpx;
    margin-left: 34rpx;
    margin-right: 30rpx;
}

.list,.listac {
    font-family: PingFang SC;
}

.listac {
    color: #303030;
    font-size: 34rpx;
    font-weight: 700;
    position: relative;
}

.listac::before {
    background: #ffc729;
    border-radius: 4rpx;
    bottom: 0;
    content: "";
    height: 8rpx;
    left: 50%;
    position: absolute;
    transform: translateX(-50%);
    width: 86rpx;
}

.content {
    box-sizing: border-box;
    position: relative;
}

.smalltit {
