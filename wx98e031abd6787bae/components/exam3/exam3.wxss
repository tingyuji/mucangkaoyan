}

.xuanlist {
    align-items: center;
    background: #fff;
    border: 1px solid hsla(0,0%,58%,.22);
    border-radius: 10rpx;
    box-sizing: border-box;
    color: #313131;
    display: flex;
    font-size: 32rpx;
    margin-top: 38rpx;
    padding: 46rpx 42rpx;
    position: relative;
    width: 100%;
}

.xuanlistac {
    background: #fff9eb;
